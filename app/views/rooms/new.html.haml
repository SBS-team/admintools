%h1 New room

= form_for @room do |f|
  -if @room.errors.any?
    #error_explanation
      %h2= "#{pluralize(@room.errors.count, "error")} prohibited this photo from being saved:"
      %ul
        - @room.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :office
    = f.text_field :office
  -#.field
  -#  = f.label :responsible
  -#  = f.text_field :responsible
  .actions
    = f.submit 'Save'